    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "preLaunchTask": "2. Build",
            "name" : "Launch",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/build/peldd.exe",
            "args": [
                "-t", 
                "${env:MINGW64_ROOT}/bin/Qt5Core.dll", 
                "${env:MINGW64_ROOT}/bin/Qt5Gui.dll"
            ],
            "miDebuggerPath": "${env:MINGW64_ROOT}/bin/gdb.exe"
        }
    ]
}